{
  "ui:page": {
    "onload": {
      "xhr:datasource": {
        "url": "https://xmie-ot-backend.mdbgo.io/api/registration",
        "method": "GET",
        "headers": {},
        "payload": {},
        "map:results": "#/definitions/results"
      }
    },
    "onsubmit": {
      "xhr:datasource": {
        "url": "https://xmie-ot-backend.mdbgo.io/api/registration",
        "method": "PUT",
        "headers": {},
        "map:payload": {
          "name" :"${ownData.firstName}",
          "Lag" : "${ownData.lag}",
          "address" : "${ownData.adress}",
          "city" : "${ownData.city}",
          "zip" : "${ownData.zip}",
          "ownphone" : "${ownData.ownphone}",
          "ownemail" : "${ownData.ownemail}",
          "sizes": {
            "Skirt" : "${sizes.skirt}",
            "pants" : "${sizes.pants}",
            "socks" : "${sizes.socks}"
          },
          "person": {
            "momname" : "${parents.momName}",
            "momephone" : "${parents.momPhone}",
            "momemail" : "${parents.momsEmail}",
            "dadname" : "${parents.dadName}",
            "dadphone" : "${parents.dadPhone}",
            "dadmail" : "${parents.dadEmail}"
          }
        },
        "map:results": "#/definitions/results"
      }
    }
  },
  "properties": {
    "array": {
      "xhrSelectTest": {
        "onload": {
          "xhr:datasource": {
            "url": "https://xmie-ot-backend.mdbgo.io/api/registration",
            "method": "GET",
            "headers": {},
            "payload": {},
            "map:items.enum": "name"
          }
        }
      }
    }
  },
  "definitions": {
    "results": {
      "string": {
        "firstName": "${string.first_name}",
        "lastName": "${string.last_name}",
        "select": "${string.select}",
        "react-select": "${string.react_select}",
        "upload": "${string.upload}",
        "bio": "${string.bio}",
        "date": "${string.date}",
        "password": "${string.password}",
        "telephone": "${string.telephone}"
      },
      "integer": {
        "customRating": "${integer.custom_rating}",
        "age": "${integer.age}"
      },
      "number": {
        "currency": "${number.currency}"
      },
      "boolean": {
        "default": "${boolean.default}",
        "radio": "${boolean.radio}"
      },
      "array": {
        "multiSelect": "${array.multiSelect}",
        "creatableSelectTest": "${array.creatable_select_test}",
        "selectTest": "${array.select_test}"
      },
      "object": {
        "customComponent": "${object.custom_component}"
      }
    }
  }
}